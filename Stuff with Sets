aSet = {"Luffy", "Zoro", "Usopp", "Sanji", "Nami", "Chopper"}

bSet = {"Robin", "Franky", "Brook", "Luffy", "Nami"}

newSet = aSet - bSet
print(newSet)
newSet = bSet - aSet
print(newSet)
newSet = aSet ^ bSet
print(newSet)
